        <input type="password" id="password" name="password" class="form-control m-1" value="" placeholder="Password"
          autocomplete="off" />
        <div>
          <input type="checkbox" id="show-password"> Mostrar Contrase√±a
        </div>
      </div>
      <div class=" d-flex flex-row justify-content-center text-center w-100">
        <button type="submit" id="login-btn" class="btn btn-primary  m-1 w-50">
          Registrarse
        </button>
      </div>
    </form>

    <div class=" d-flex flex-column align-items-center text-center w-100 ">
      <div><br></div>
      <button onclick="location.href='/login'" class="btn btn-success w-50 ">
        Volver a Login
      </button>
    </div>



  </div>
